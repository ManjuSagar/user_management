<div class="container">
  <div class="right">
    <%=link_to "Add Employee", new_employee_path, :class=> "btn btn-primary", :style=>"margin-top: 50px"%>
    <%=link_to "Add Task", new_task_path, :class=> "btn btn-primary", :style=>"margin-top: 50px; margin-left: 20px;"%>
  </div>
<table class="table">
  <thead>
  <tr>
    <th class="table-header">Title</th>
    <th class="table-header">Description</th>
  </tr>
  </thead>
  <tbody>
  <%@tasks.each do |task| %>
      <tr>
        <td><%=task.title%></td>
        <td><%=task.description%></td>
      </tr><br/>
  <%end%>
  </tbody>
</table>
</div>